#include <iostream>
using namespace std;
void SortDec3(double& a, double& b, double& c) {
    if (a < b) swap(a, b);
    if (b < c) swap(b, c);
    if (a < b) swap(a, b);
}

int main() {
double a1 = 1, b1 = 2, c1 = 3;
    double a2 = 4, b2 = 5, c2 = 6;
  
    SortDec3(a1, b1, c1);

    SortDec3(a2, b2, c2);

    cout << a1 << " " << b1 << " " << c1 << endl;
    cout << a2 << " " << b2 << " " << c2 << endl;

}
